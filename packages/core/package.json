{
  "name": "@bitmachina/dinero-core",
  "version": "2.0.0-alpha.9",
  "description": "Common code between Dinero.js packages",
  "keywords": [
    "money",
    "monetary",
    "amount",
    "immutable"
  ],
  "homepage": "https://github.com/johnhooks/dinero.js",
  "bugs": "https://github.com/johnhooks/dinero.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnhooks/dinero.js.git"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Sarah Dayan",
      "url": "https://sarahdayan.dev"
    },
    {
      "name": "John Hooks",
      "url": "https://johnhooks.io"
    }
  ],
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "umd:main": "dist/umd/index.production.js",
  "jsdelivr": "dist/umd/index.production.js",
  "unpkg": "dist/umd/index.production.js",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:clean": "rimraf ./{dist,temp}",
    "build:tsc": "tsc -b",
    "build:types": "api-extractor run --local --verbose",
    "build:cjs": "rollup --config",
    "build:esm": "esbuild --bundle src/index.ts --target=es2020 --format=esm --outfile=dist/esm/index.js",
    "lint": "eslint --config ../../.eslintrc.js --ignore-path ../../.eslintignore --ext .js,.ts .",
    "lint:fix": "eslint --config ../../.eslintrc.js --ext .js,.ts --fix .",
    "test": "vitest run --config=../../vitest.config.ts"
  },
  "dependencies": {},
  "devDependencies": {}
}
